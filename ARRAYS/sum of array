#include <iostream>

using namespace std;

void arryadd(int n,int a[]){
    int sum=0;
    for(int i=0;i<n;i++){
        sum+=a[i];
    }
    cout<<sum;
}
int main()
{
    int a[10];
    int n;
    cout<<"Enter the n numbers: "<<endl;
    cin>>n;
    cout<<"Enter the array numbers: "<<endl;
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    
    //declaring functions
    arryadd(n,a);
    
    
    return 0;
}
